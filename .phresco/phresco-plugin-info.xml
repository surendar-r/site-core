                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
					<multiple>true</multiple>
					<sort>false</sort>
					<show>false</show>
                    <value></value>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
